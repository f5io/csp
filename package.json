  "license": "MIT",
  "devDependencies": {
    "@types/tape": "^4.2.33",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "eslint": "^5.15.2",
    "nyc": "^13.3.0",
    "tape": "^4.10.1",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333"
  }
}
